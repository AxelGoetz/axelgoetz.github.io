---
layout: nil
---
{"data": [
{% for project in site.projects %}
  {
    "title": "{{ project.title }}",
    "project_id": "{{ project.project_id }}",
    "github_link": "{{ project.github_link }}"
    {% if project.demo_link %}
      ,"demo_link": "{{ project.demo_link }}"
    {% endif %}
  }
  {% unless forloop.last %},{% endunless %}
{% endfor %}
]}
